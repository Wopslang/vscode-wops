{
    "name": "wopslang",
    "publisher": "WopsTeam",
    "displayName": "Wops",
    "repository": {
        "type": "git",
        "url": "https://github.com/Wopslang/vscode-wops"
      },
    "description": "Syntax highlighting for Wops",
    "icon": "logo2.png",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "wops",
            "aliases": ["Wops", "wops"],
            "extensions": [".wops"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "wops",
            "scopeName": "source.wops",
            "path": "./syntaxes/wopslang.tmLanguage.json"
        }]
    }
}